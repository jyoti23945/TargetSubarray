#include<iostream>
using namespace std;
int main(){
    double x,y;
    cin>>x>>y;
     if (y==0)return 0;
    double n1=x/y;
    int n2=x/y;
    int result=0;
    
    if((n1-n2)<=0.5){
         result=y*n2;
        cout<<result;
    }
    else if(n1-n2>0.5){
         result=n2+1;
         cout<<y*(n2+1);
        }
        return 0;

    }
