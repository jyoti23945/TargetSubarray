EASY C++ SOLUTION
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of the array"<<endl;
    cin>>n;
    vector<int>v(n);
    vector<string>s;
    for(int i=0;i<n;i++)
    {
        cin>>v[i];
    }
    for(int i=0;i<n;i++)
    {
        if(v[i]%3==0 && v[i]%5==0)
        s.push_back("ThreeFive");
        else if(v[i]%3==0)
        s.push_back("Three");
        else if(v[i]%5==0)
        s.push_back("Five");
        else
        {
            int ans=v[i];
         string temp=to_string(ans);
         s.push_back(temp);
        }
    }
    for(int i=0;i<s.size();i++)
    {
        cout<<s[i]<<" ";
    }
   return 0;
