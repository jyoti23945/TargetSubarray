#include<iostream>
#include<cctype>
using namespace std;
string valuetochar(string input, int val){
    string result="";
    for(auto ch:input){
    if(isalpha(ch)){
        if(islower(ch)){
           result+=((ch-'a'+val)%26)+'a';

        }
        else{
            result+=((ch-'A'+val)%26)+'A';
        }
        }else if(isdigit(ch)){
            result+=((ch-'0'+val)%10)+'0';
        }
        else{
            if(ch=='@'){
                result+='#';
            }
            else{
                result+='@';
            }
        }
        }
        return result;

    }
    int main(){
        string input;
        int value;
        cin>>input;
        cin>>value;
        string output=valuetochar(input,value);
        cout<<output;
        return 0;
    }
